<h2>Edit a User</h2>
<form action="/users/<%= "#{@user.id}" %>" method='post'id='edit'>
  <label for="user[name]">Name:</label>
  <input type="text" name="user[name]" id="name" "<%= "#{@user.name}" %>">
  <label for="user[email]" >Email:</label>
  <input type="text" name="user[email]" id="user" value="<%= "#{@user.email}" %>">
  <input type="submit" value="Submit Fact">
</form>